{
    "name": "podhost/podstream",
    "description": "Podcast management scaffolding for Podhost app",
    "keywords": ["laravel", "podcast"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/podhost/podstream/issues",
        "source": "https://github.com/podhost/podstream"
    },
    "authors": [
        {
            "name": "Goran Krgovic",
            "email": "goran@digitalinfinity.rs"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "illuminate/support": "^8.0",
        "laravel/jetstream": "^2.3",
        "laravel/fortify": "^1.6.1",
        "inertiajs/inertia-laravel": "^0.3",
        "goldspecdigital/laravel-eloquent-uuid": "^8.0"
    },
    "require-dev": {
        "laravel/sanctum": "^2.6",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Podhost\\Podstream\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Podhost\\Podstream\\Tests\\": "tests/",
            "App\\": "stubs/app/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Podhost\\Podstream\\PodstreamServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
